<div class="page-header">
  <%= link_to power_unit_states_path(@power_unit), :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_power_unit_state_path(@power_unit, @state), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show STATE</h1>
</div>

<dl class="dl-horizontal">
  <dt>Raw value:</dt>
  <dd><%= @state.raw_value %></dd>

  <dt>State:</dt>
  <dd><%= @state.state %></dd>

</dl>
