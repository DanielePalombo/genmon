<div class="page-header">
  <%= link_to power_unit_red_alarms_path(@power_unit), :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_power_unit_red_alarm_path(@power_unit, @red_alarm), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show red ALARM</h1>
</div>

<dl class="dl-horizontal">
  <dt>Raw value:</dt>
  <dd><%= @red_alarm.raw_value %></dd>

  <dt>State:</dt>
  <dd><%= @red_alarm.state %></dd>

</dl>
